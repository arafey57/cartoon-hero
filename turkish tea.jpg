button.newsletter_button {
	border: none;
	outline: none;
	background-color: #1fa2be;
	color: white;
	cursor: pointer;
	padding: 15px;
	border-radius: 0px;
}

button.sticky_bottom {
	position: fixed;
	bottom: 20px;
	right: 20px;
	z-index: 99;
	border-radius:10px;
	padding:30px;
	display:flex;
	align-items:center;
	justify-content:center;
	width:auto;
}
button.sticky_bottom i {
font-size:2em;
padding-right:20px;
margin-right:20px;
border-right:1px solid rgba(255,255,255,.4);
}
button.sticky_bottom span {
position: absolute;
right: -10px;
top: -10px;
background: white;
padding: 20px;
border-radius: 100%;
width: 20px;
height: 20px;
display: flex;
align-items: center;
justify-content: center;
color: black;
font-size: 1.5em;
font-weight: 900;
transition:.2s all ease;
border:5px solid #1fa2be;
}

button.sticky_bottom span:hover {
transform:rotate(-90deg) scale(1.2);
}
button.sticky_button i {
	font-size: 2.4em;
}


